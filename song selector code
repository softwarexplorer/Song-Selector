		if (!answer.equalsIgnoreCase("yes") && !answer.equalsIgnoreCase("no")) {

			answer = JOptionPane
					.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs? ");
		}
		if (answer.equalsIgnoreCase("no")) {
			JOptionPane
					.showInputDialog("That's alright not everyone likes the 90's.\nLet's move on to the next part of the survey. (ENTER)");
			System.out.println();
		}
		while (answer.equalsIgnoreCase("yes")) {

			answer = JOptionPane
					.showInputDialog("Enter the number of the song: ");

			if (!answer.equalsIgnoreCase("1") && !answer.equalsIgnoreCase("2")) {
				answer = JOptionPane
						.showInputDialog("Error! You must enter either: 1 or 2\nWould you like to try again(enter yes or no)?");

			}
			if (answer.equalsIgnoreCase("1")) {
				songlist.add(ninety1);
				answer = JOptionPane
						.showInputDialog("You have added '"
								+ ninety1
								+ "' to your music list.\nWould you like to add the other song as well(enter yes or no)?");
				ninety.remove(ninety1);
				count++;
				while (!answer.equalsIgnoreCase("yes")
						&& !answer.equalsIgnoreCase("no")) {
					answer = JOptionPane
							.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs(enter yes or no)? ");

				}
				if (answer.equalsIgnoreCase("yes")) {
					songlist.add(ninety2);
					ninety.remove(ninety2);
					count++;
					break;
				}

			}
			if (answer.equalsIgnoreCase("2")) {
				songlist.add(ninety2);
				answer = JOptionPane
						.showInputDialog("You have added '"
								+ ninety2
								+ "' to your music list.\nWould you like to add the other song as well(enter yes or no)?");
				count++;
				while (!answer.equalsIgnoreCase("yes")
						&& !answer.equalsIgnoreCase("no")) {
					answer = JOptionPane
							.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs(enter yes or no)? ");

				}
				if (answer.equalsIgnoreCase("yes")) {
					songlist.add(ninety1);
					count++;
					ninety.remove(ninety1);
					break;
				}

			}
			i = 0;
			n = 1;
			System.out.print(name1 + " Songlist:  \n");
			while (i < songlist.size()) {
				while (n <= count) {

					System.out.println(n + ". " + songlist.get(i));
					i++;
					n++;
				}
			}
			System.out.println();

			JOptionPane
					.showInputDialog("Ok, the songs in your songlist now are: \n"
							+ songlist + "\n (ENTER)");

		}

		answer = JOptionPane
				.showInputDialog("Here's some songs of the 2000's: \n"
						+ twothousand
						+ "\nDo you like any of these(enter yes or no)? ");
		i = 0;
		n = 1;
		System.out.print("Twothousands music list:  \n");
		while (i < twothousand.size()) {
			while (n <= 5) {

				System.out.println(n + ". " + twothousand.get(i));
				i++;
				n++;
			}
		}
		System.out.println();

		while (!answer.equalsIgnoreCase("yes") && !answer.equalsIgnoreCase("no")) {

			answer = JOptionPane
					.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs? ");
		}
		if (answer.equalsIgnoreCase("no"))
			JOptionPane
					.showInputDialog("That's alright not everyone likes the 2000's.\nLet's move on to the next part of the survey. (ENTER)");

		while (answer.equalsIgnoreCase("yes")) {

			answer = JOptionPane
					.showInputDialog("Enter the number of the song: ");

			if (!answer.equalsIgnoreCase("1") && !answer.equalsIgnoreCase("2")
					&& !answer.equalsIgnoreCase("3")
					&& !answer.equalsIgnoreCase("4")
					&& !answer.equalsIgnoreCase("5")) {
				answer = JOptionPane
						.showInputDialog("Error! You must enter either: 1, 2, 3, 4, or 5\nWould you like to try again(enter yes or no)?"
								+ " (ENTER)");

			}
			if (answer.equalsIgnoreCase("1")) {
				songlist.add(twothousand1);
				answer = JOptionPane
						.showInputDialog("You have added '"
								+ twothousand1
								+ "' to your music list.\nWould you like to add another song(enter yes or no)? ");
				twothousand.remove(twothousand1);
				count++;
				while (!answer.equalsIgnoreCase("yes")
						&& !answer.equalsIgnoreCase("no")) {
					answer = JOptionPane
							.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs(enter yes or no)? ");

				}
			}
			if (answer.equalsIgnoreCase("2")) {
				songlist.add(twothousand2);
				answer = JOptionPane
						.showInputDialog("You have added '"
								+ twothousand2
								+ "' to your music list.\nWould you like to add another song(enter yes or no)? ");
				twothousand.remove(twothousand2);
				count++;
				while (!answer.equalsIgnoreCase("yes")
						&& !answer.equalsIgnoreCase("no")) {
					answer = JOptionPane
							.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs(enter yes or no)? ");

				}
			}
			if (answer.equalsIgnoreCase("3")) {
				songlist.add(twothousand3);
				answer = JOptionPane
						.showInputDialog("You have added '"
								+ twothousand3
								+ "' to your music list.\nWould you like to add another song(enter yes or no)? ");
				twothousand.remove(twothousand3);
				count++;
				while (!answer.equalsIgnoreCase("yes")
						&& !answer.equalsIgnoreCase("no")) {
					answer = JOptionPane
							.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs(enter yes or no)? ");

				}
			}
			if (answer.equalsIgnoreCase("4")) {
				songlist.add(twothousand4);
				answer = JOptionPane
						.showInputDialog("You have added '"
								+ twothousand4
								+ "' to your music list.\nWould you like to add another song(enter yes or no)? ");
				twothousand.remove(twothousand4);
				count++;
				while (!answer.equalsIgnoreCase("yes")
						&& !answer.equalsIgnoreCase("no")) {
					answer = JOptionPane
							.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs(enter yes or no)? ");

				}
			}
			if (answer.equalsIgnoreCase("5")) {
				songlist.add(twothousand5);
				answer = JOptionPane
						.showInputDialog("You have added '"
								+ twothousand5
								+ "' to your music list.\nWould you like to add another song(enter yes or no)? ");
				twothousand.remove(twothousand5);
				count++;
				while (!answer.equalsIgnoreCase("yes")
						&& !answer.equalsIgnoreCase("no")) {
					answer = JOptionPane
							.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs(enter yes or no)? ");

				}
			}

			if (answer.equalsIgnoreCase("no")) {
				i = 0;
				n = 1;
				System.out.print(name1 + " Songlist:  \n");
				while (i < songlist.size()) {
					while (n <= count) {

						System.out.println(n + ". " + songlist.get(i));
						i++;
						n++;
					}
				}
				JOptionPane.showInputDialog("Ok, let's move on. (ENTER)");
			}
		}

		System.out.println();

		answer = JOptionPane
				.showInputDialog("Are there any more songs that you like that you would like to add (enter yes or no)? ");
		if (!answer.equalsIgnoreCase("yes") && !answer.equalsIgnoreCase("no")) {
			answer = JOptionPane
					.showInputDialog("Error, You must Enter yes or no.\nAre there any more songs that you like that you would like to add (enter yes or no)?");

		}
		if (answer.equalsIgnoreCase("no")) {
			JOptionPane
					.showInputDialog("Ok.\nLet's move on to the next part of the survey. (ENTER)");

		}
		while (answer.equalsIgnoreCase("yes")) {
			answer = JOptionPane.showInputDialog("Enter Title Here: ");
			songlist.add(answer);
			count++;
			i = 0;
			n = 1;
			System.out.print(name1 + " Songlist:  \n");
			while (i < songlist.size()) {
				while (n <= count) {

					System.out.println(n + ". " + songlist.get(i));
					i++;
					n++;
				}
			}
			answer = JOptionPane
					.showInputDialog("You have added '"
							+ answer
							+ "' to your music list.\nWould you like to add another song? ");
			if (!answer.equalsIgnoreCase("yes")
					&& !answer.equalsIgnoreCase("no")) {
				answer = JOptionPane
						.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs(enter yes or no)? ");

			}
		}
		
		System.out.println();

		JOptionPane.showInputDialog("The songs in your songlist are now: \n"
				+ songlist + "\n (ENTER)");
		String lastName = JOptionPane.showInputDialog("Enter your last name: ");
		String middleIntitial = JOptionPane.showInputDialog("Enter your middle initial: ");
		answer = JOptionPane.showInputDialog("\t\t\tNew Message\n\n\n\n"
				+ " To: Audio Engineering <aes-l@mtu.edu>\n"
				+ " --------------------------------------\n"
				+ " Cc:                                  \n"
				+ " --------------------------------------\n"
				+ " Subject:                             \n"
				+ " --------------------------------------\n"
				+ " From: " + name + " " + middleIntitial + ". " +  lastName + " - " + name.substring(0, 1).toLowerCase()+ 
				middleIntitial.toLowerCase() + ". " +lastName.toLowerCase() + "@mtu.edu  \n"
				+ " --------------------------------------\n"
				+ " "+name1 + " Songlist.txt\t\t\t\t\t\tWould you like to send?\n"
						+ "\t20KB");
		if (!answer.equalsIgnoreCase("yes")
				&& !answer.equalsIgnoreCase("no")) {
			answer = JOptionPane
					.showInputDialog("Error, You must Enter yes or no.\nDo you like any of these songs(enter yes or no)? ");

		}
		while (answer.equalsIgnoreCase("yes")){
			JOptionPane.showInputDialog("Email Sent!");
			break;
		}
		while (answer.equalsIgnoreCase("no")){
			JOptionPane.showInputDialog("Ok, " + name1 + " Songlist is saved.");
			break;
		}
	
		}
		
