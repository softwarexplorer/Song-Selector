		System.out.println();

		System.out.println();

		answer = JOptionPane
				.showInputDialog("Are there any more songs that you like that you would like to add (enter yes or no)? ");
		if (!answer.equalsIgnoreCase("yes") && !answer.equalsIgnoreCase("no")) {
			JOptionPane
					.showInputDialog("Error, You must Enter yes or no. (ENTER)");

			answer = JOptionPane
					.showInputDialog("Are there any more songs that you like that you would like to add (enter yes or no)? ");
		}
		if (answer.equalsIgnoreCase("no")) {
			JOptionPane
					.showInputDialog("Ok.\nLet's move on to the next part of the survey. (ENTER)");
			System.out.println();
		}
		while (answer.equalsIgnoreCase("yes")) {
			answer = JOptionPane.showInputDialog("Enter Title Here: ");
			songlist.add(answer);
			JOptionPane.showInputDialog("You have added '" + answer
					+ "' to your music list. (ENTER)");
			System.out.println();
			answer = JOptionPane
					.showInputDialog("Would you like to add another song? ");

		}

		JOptionPane.showInputDialog("The songs in your songlist are now: \n"
				+ songlist + "\n (ENTER)");

		System.out.println();
		new ArrayList<String>();
		ArrayList<String> templist = new ArrayList<String>();

		for ( i = 0; i < songlist.size(); i++) {

			String titleplacement = JOptionPane
					.showInputDialog("Enter the title of the song you want (in exact case and spacing)\n in order from top "
							+ "(all time favorite) to bottom (last favorite): ");
			templist.add(titleplacement);
			songlist.remove(titleplacement);
			for (int line1 = 0; line1 < songlist.size(); line1++) {
				JOptionPane.showInputDialog(songlist.get(line1));
			}
		}
		songlist = templist;
		JOptionPane.showInputDialog("This is your new Songlist: \n" + songlist
				+ "\n (ENTER)");

	}

}
